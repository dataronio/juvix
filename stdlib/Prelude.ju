mod Prelude where

-- it seems custom arrow syntax is not operational

let ty_ = %Builtin.Type

sig ty : ty_ 1
let ty = ty_ 0

let SAny = %Builtin.SAny
let arr = %Builtin.Arrow
let dpair = %Builtin.Pair

let (->) = arr SAny
declare infixr (->) 1

let (:) = %Builtin.Colon
declare infixr (:) 2

sig id : (a : ty) -> a -> a
let id _ x = x

sig const : (a : ty) -> (b : ty) -> a -> b -> a
let const _ _ x = \_ -> x

