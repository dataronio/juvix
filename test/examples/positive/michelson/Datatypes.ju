mod Datatypes where

open Prelude
open Michelson

sig (+) : nat -> nat -> nat
let (+) = %Michelson.add
declare infixl (+) 5

sig nil : list operation
let nil = %Michelson.nil

sig make-pair : list operation -> nat -> pair (list operation) nat
let make-pair = %Michelson.pair

sig main : pair nat nat -> pair (list operation) nat
let main =
  \_ -> make-pair nil 0
